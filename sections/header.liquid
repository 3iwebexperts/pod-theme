<div data-section-id="header-logo-center" data-section-type="header">
  <header class="header is-fixed show-on-scroll" role="banner" data-header="">
  {%- liquid
   

      if section.settings.logo_position == 'top-center' or section.settings.menu == blank
        render 'header-search', input_id: 'Search-In-Modal-1'
      endif
    -%}
    <div class="header-container">
      <div class="header-nav">
        <div class="header-nav__menu">
          <div class="header-nav__menu-item">
            <a href="#" id="hamburger_menu" class="hamburger" data-mobile-menu-toggle="">
              <span class="hamburger-box">
                <span class="hamburger-inner"></span>
              </span>
              <span class="hamburger-text hide-on-mobile">Menu</span>
            </a>
          </div>
          <div class="header-nav header-nav--center header-nav--logo">
            {% if section.settings.image != blank %}
              <a href="/" itemprop="url" class="header-logo">
                <img
                  src="{{ section.settings.image | img_url: "master" }}"
                  alt="{{ section.settings.image.alt }}"
                  class="header-logo__image"
                >
              </a>
            {% endif %}
          </div>
          <div>
            <div class="header-nav__menu-item" data-search-open-button="">
               {% render 'header-search', input_id: 'Search-In-Modal' %}
              <a href="/search">
              <svg
                aria-hidden="true"

                focusable="false"
                role="presentation"
                class="icon icon-search"
                viewBox="0 0 33 34"
              >
                <g id="magnify-icon" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                  <g id="Search-Icon" fill="#FFFFFF">
                    <polygon id="Fill-1" points="29.1137219 33.3247229 20.0974919 24.3084929 17.9702219 22.1812229 21.7459019 18.4048529 32.8900919 29.5483529"></polygon>
