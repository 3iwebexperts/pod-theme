{% comment %} <div class="header__filler" data-header-filler="" style="height: 70px;"></div> {% endcomment %}
  <div class="navigation" {% comment %}style="top: 70px; height: calc(100% - 70px);"{% endcomment %}>
    <div class="navigation-body">
      <div class="navigation-body__container">
        <div class="navigation-categories swiper swiper-container swiper-initialized swiper-horizontal swiper-pointer-events swiper-free-mode swiper-backface-hidden">
          <div class="swiper-wrapper tabs-list">
            {% for block in section.blocks %}
              {% if block.settings.title != blank %}
              <a href="#tab_{{ block.id }}" class="swiper-slide navigation-categories__button {% if forloop.first %}navigation-categories__button--active{% endif %}">
              {{- block.settings.title -}}
              </a>
              {% endif %}
            {% endfor %}
            {% comment %}
            {% if section.settings.tab_title_1 != blank %}
              <a href="#tab1" class="swiper-slide navigation-categories__button navigation-categories__button--active">
                {{- section.settings.tab_title_1 -}}
              </a>
            {% endif %}
            {% if section.settings.tab_title_2 != blank %}
              <a href="#tab2" class="swiper-slide navigation-categories__button">
                {{ section.settings.tab_title_2 -}}
              </a>
            {% endif %}
            {% if section.settings.tab_title_3 != blank %}
              <a href="#tab3" class="swiper-slide navigation-categories__button">
                {{ section.settings.tab_title_3 -}}
              </a>
            {% endif %}
            {% if section.settings.tab_title_4 != blank %}
              <a href="#tab4" class="swiper-slide navigation-categories__button">
                {{ section.settings.tab_title_4 -}}
              </a>
            {% endif %}
            {% if section.settings.tab_title_5 != blank %}
              <a href="#tab5" class="swiper-slide navigation-categories__button">
                {{ section.settings.tab_title_5 -}}
              </a>
            {% endif %}
            {% endcomment %}
          </div>
        </div>
        <div class="navigation-blocks">
          {% content_for "blocks" %}
          {% if section.settings.tab_title_1 != blank %}
            <div id="tab1" class="tab_inner active" style="display:block;">
              <div data-shopify-editor-block="" class="card-grid">
                {% if section.settings.tab_grid_title_1 != blank %}
                  <div class="card-grid__title">{{ section.settings.tab_grid_title_1 }}</div>
                {% endif %}
                <div class="card-grid__grid">
                  {% for block in section.blocks %}
                    {% if block.type == 'tab_one_grid' %}
                      {% assign collection = block.settings.tab_1_collection %}
                      {% if collection != blank %}
                        <a
                          href="{{ collection.url }}"
                          class="image_card {% if block.settings.enable_highlight_tab1 != blank %}image_card--highlighted{% endif %}"
                        >
                          <div class="image-card__image-container frame frame--1x1">
                            {% if collection.image != blank %}
                              <img
                                src="{{ collection.image | img_url: 'master' }}"
                                srcset="{{ collection.image | img_url: 'master' }}"
                                sizes="(max-width: 450px) 50w, (max-width: 992px) 176px, 148px"
                                loading="lazy"
                                alt="{{ collection.title }}"
                                class="frame__inner"
                              >
                            {% else %}
                              <img
                                src="{{ 'placeholder.png' | asset_url }}"
                                srcset="{{ 'placeholder.png' | asset_url }}"
                                sizes="(max-width: 450px) 50w, (max-width: 992px) 176px, 148px"
                                loading="lazy"
                                alt="{{ collection.title }}"
                                class="frame__inner"
                              >
                            {% endif %}
                          </div>
                          <div class="image-card__title">
                            {% if block.settings.highlight_text_tab1 != blank -%}
                              <b class="image-card__highlight-text">{{ block.settings.highlight_text_tab1 }}</b>
                            {%- endif %}
                            {{ collection.title }}
                          </div>
                        </a>
                      {% endif %}
                    {% endif %}
                  {% endfor %}
                </div>
              </div>
              {% if section.settings.tab_button_caption_1 != blank and section.settings.tab_button_link_1 != blank %}
                <a
                  href="{{ section.settings.tab_button_link_1 }}"
                  target=""
                  data-shopify-editor-block=""
                  class="cta  cta--bottom-space"
                  ><span class="cta__label"> {{ section.settings.tab_button_caption_1 }} </span>
                  <div class="cta__icon"><span class="cta__arrow-icon"></span></div>
                </a>
              {% endif %}
              <div class="navigation-carousel">
                {% if section.settings.tab_slider_title_1 != blank %}
                  <div class="navigation-carousel__title">{{ section.settings.tab_slider_title_1 }}</div>
                {% endif %}
                <div class="navigation-carousel__slider swiper swiper-container swiper-initialized swiper-horizontal swiper-pointer-events swiper-free-mode swiper-backface-hidden">
                  <div class="swiper-wrapper" style="transform: translate3d(0px, 0px, 0px);">
                    {% for block in section.blocks %}
                      {% if block.type == 'tab_one_slider' %}
                        <a
                          href="{{ block.settings.tab_1_link }}"
                          class="navigation-carousel__card swiper-slide"
                          style="width: 256.111px; margin-right: 15px;"
                        >
                          <div class="navigation-carousel__image-container frame frame--16x9">
                            {% if block.settings.tab_1_image != blank %}
                              <img
                                src="{{ block.settings.tab_1_image | img_url:'master' }}"
                                srcset="{{ block.settings.tab_1_image | img_url:'master' }}"
                                sizes="(max-width: 450px) 80w, (max-width: 991px) 310px, 257px"
                                loading="lazy"
                                alt="{{ block.settings.tab_1_image.alt }}"
                                class="frame__inner navigation-carousel__image"
                              >
                            {% else %}
                              <img
                                src="{{ 'placeholder.png' | asset_url }}"
                                srcset="{{ 'placeholder.png' | asset_url }}"
                                sizes="(max-width: 450px) 80w, (max-width: 991px) 310px, 257px"
                                loading="lazy"
                                alt="placeholder"
                                class="frame__inner navigation-carousel__image"
                              >
                            {% endif %}
                          </div>

                          {% if block.settings.tab_1_text != blank %}
                            <div class="navigation-carousel__card-title">
                              {{ block.settings.tab_1_text }}
                            </div>
                          {% endif %}
                        </a>
                      {% endif %}
                    {% endfor %}
                  </div>
                  <div class="navigation-carousel__arrows-container">
                    <div data-arrow-prev="true" class="navigation-carousel__arrow--prev swiper-button-disabled"></div>
                    <div data-arrow-next="true" class="navigation-carousel__arrow--next"></div>
                  </div>
                </div>
              </div>
            </div>
          {% endif %}
