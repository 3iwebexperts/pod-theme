                    href="{{ block.settings.tab_3_link }}"
                    id="18c7c6df-1835-47e3-85f8-bd070170bb21"
                    data-shopify-editor-block=""
                    class="card-block   "
                  >
                    <div class="card-block__content">
                      <div class="card-block__image-container frame frame--16x9">
                        {% if block.settings.tab_3_image != blank %}
                          <img
                            src="{{ block.settings.tab_3_image | img_url: "master" }}"
