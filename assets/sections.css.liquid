/* SECTIONS : FAQ start */
.faq_box {
  background: #f5f5f6 !important;
}
.faq_box .accordion-box.rounded.bg-secondary {
  border-radius: 12px;
  border-radius: 12px;
  padding: 25px 40px;
}
.faq_box .accordion.group {
  margin: 0;
  border-bottom: 1px solid rgba(26, 26, 26, 0.12);
  border-radius: 0;
  padding: 15px 0;
  background: transparent;
}
.faq_box .accordion.group:last-child {
  border: none;
}
.faq_box .accordion__toggle {
  font-size: 16px;
  color: #1a1a1a;
  font-family: Sequel Sans Title;
  letter-spacing: 0.7px;
  cursor: pointer;
  align-items: center;
  line-height: 1;
  gap: 0.5rem;
}
.faq_box .accordion__toggle:hover .circle-chevron {
  background: #000;
  color: #fff;
}
.faq_box .accordion__content {
  padding-top: 15px;
  padding-bottom: 15px;
  padding-inline-end: 1.5rem;
}
.faq_box .circle-chevron {
  width: 26px;
  height: 26px;
  border-radius: 9999px;
  background: #1a1a1a1a;
  flex-shrink: 0;
  place-items: center;
  transition: all 0.2s ease-in-out;
  display: grid;
  cursor: pointer;
}
.faq_box .circle-chevron .fa {
  font-size: 12px;
}
.gap-10 {
  gap: 1.5rem;
}
.gap-4 {
  gap: 1rem;
}
.v-stack {
  display: grid;
}
.bg-secondary {
  background-color: #424446 !important;
  background: #f3f3f3 !important;
}
.rounded {
  border-radius: var(--rounded);
}
.faq-availability {
  gap: 1.5rem;
  text-align: center;
  justify-items: center;
  display: grid;
}
.accordion {
  background-color: #f5f5f6;
  border: 2px solid transparent;
  border-radius: 10px;
  padding: 15px 20px;
  transition: none;
}
.accordion:not(:first-child) {
  margin-top: 10px;
}
.prose >:first-child {
  margin-block-start: 0 !important;
}
.prose >:last-child {
  margin-block-end: 0 !important;
}
.section-stack__intro .faq-availability {
  text-align: start;
  justify-items: start;
}
.accordion__toggle {
  align-items: flex-start;
  background-color: transparent;
  border: none;
  display: flex;
  justify-content: space-between;
  margin: 0;
  padding: 0;
  width: 100%;
}
span.text-subdued {
  color: #6e6e6ecf;
}
@media screen and (min-width: 1200px) {
  .section-stack__intro {
    width: 50%;
  }
  .section-stack__main {
    width: 50%;
  }
}
@media (min-width: 768px) {
  .accordion {
    border: 3px solid transparent;
    padding: 42px 50px;
  }
  .accordion:not(:first-child) {
    margin-top: 20px;
  }
}

/* SECTIONS : FAQ end */