/* SECTIONS : FAQ start */
.faq_box {
  background: #f5f5f6 !important;
}
.faq_box {
  background: #f5f5f6 !important;
}
.faq_box .circle-chevron {
  width: 26px;
  height: 26px;
  border-radius: 9999px;
  background: #1a1a1a1a;
  flex-shrink: 0;
  place-items: center;
  transition: all 0.2s ease-in-out;
  display: grid;
  cursor: pointer;
}
.faq_box .accordion.group:last-child {
  border: none;
}
.faq_box .accordion__content {
  padding-top: 15px;
  padding-bottom: 15px;
  padding-inline-end: 1.5rem;
}
.gap-10 {
  gap: 1.5rem;
}
.section-stack__intro .faq-availability {
  text-align: start;
  justify-items: start;
}
.gap-4 {
  gap: 1rem;
}
.faq-availability {
  gap: 1.5rem;
  text-align: center;
  justify-items: center;
  display: grid;
}
.v-stack {
  display: grid;
}
span.text-subdued {
  color: #6e6e6ecf;
}
@media (max-width: 1199px) {
  .faq_box .icon_text.desk {
    display: none;
  }
  .faq_box .icon_text.mob {
    display: block !important;
  }
  .faq_box .section-stack__main {
    width: 100%;
  }
  .faq_box .v-stack {
    display: grid;
    justify-items: center;
  }
}
@media (max-width: 767px) {
  .faq_box {
    padding: 60px 0;
    background: #fff !important;
  }
  .faq_box .accordion__toggle {
    font-size: 13px;
  }
}
/* SECTIONS : FAQ end */