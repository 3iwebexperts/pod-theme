}
.faq_box {
  background: #f5f5f6 !important;
}
.faq_box .circle-chevron {
  width: 26px;
  height: 26px;
  border-radius: 9999px;
  background: #1a1a1a1a;
  flex-shrink: 0;
  place-items: center;
  transition: all 0.2s ease-in-out;
  display: grid;
  cursor: pointer;
}
.faq_box .accordion.group:last-child {
  border: none;
}
.faq_box .accordion__content {
  padding-top: 15px;
  padding-bottom: 15px;
  padding-inline-end: 1.5rem;
}
.gap-10 {
  gap: 1.5rem;
}
.section-stack__intro .faq-availability {
  text-align: start;
  justify-items: start;
}
.gap-4 {
  gap: 1rem;
}
.faq-availability {
  gap: 1.5rem;
  text-align: center;
  justify-items: center;
  display: grid;
}
.faq_box .accordion__toggle {
  font-size: 16px;
  color: #1a1a1a;
  font-family: Sequel Sans Title;
  letter-spacing: 0.7px;
  cursor: pointer;
  align-items: center;
  line-height: 1;
  gap: 0.5rem;
}
.faq_box .accordion__toggle:hover .circle-chevron {
  background: #000;
  color: #fff;
}
.faq_box .accordion.group {
  margin: 0;
  border-bottom: 1px solid rgb(26 26 26 / 12%);
  border-radius: 0;
