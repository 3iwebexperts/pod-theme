/* BLOCKS : Heading start */
.h1,
.h2,
.h3,
.h4,
.h5,
.h6,
h1,
h2,
h3,
h4,
h5,
h6 {
  font-weight: 400;
}
.h1,
h1 {
  font-family: Sequel Sans Title, sans-serif;
  font-size: 32px;
  line-height: 1;
  text-transform: uppercase;
}
@media (min-width: 992px) {
  .h1,
  h1 {
    font-size: 50px;
  }
}
@media (min-width: 1200px) {
  .h1,
  h1 {
    font-size: 78px;
  }
}
.h2,
h2 {
  font-family: Sequel Sans title, sans-serif;
  font-size: 28px;
  line-height: 1;
  text-transform: uppercase;
}
@media (min-width: 992px) {
  .h2,
  h2 {
    font-size: 44px;
  }
}
@media (min-width: 1200px) {
  .h2,
  h2 {
    font-size: 68px;
  }
}
.h3,
h3 {
  font-family: Sequel Sans Title, sans-serif;
  font-size: 26px;
  line-height: 1;
}
@media (min-width: 992px) {
  .h3,
  h3 {
    font-size: 41px;
  }
}
@media (min-width: 1200px) {
  .h3,
  h3 {
    font-size: 63px;
  }
}
.h4,
h4 {
  font-family: Sequel Sans Title, sans-serif;
  font-size: 24px;
  line-height: 1;
}
@media (min-width: 992px) {
  .h4,
  h4 {
    font-size: 38px;
  }
}
@media (min-width: 1200px) {
  .h4,
  h4 {
    font-size: 60px;
  }
}
.h5,
h5 {
  font-family: Sequel Sans Title, sans-serif;
  font-size: 18px;
  line-height: 1;
}
@media (min-width: 992px) {
  .h5,
  h5 {
    font-size: 28px;
  }
}
@media (min-width: 1200px) {
  .h5,
  h5 {
    font-size: 44px;
  }
}
.h6,
h6 {
  font-size: 15px;
  line-height: 17px;
  font-family: Sequel Sans Title, sans-serif;
}
@media (min-width: 992px) {
  .h6,
  h6 {
    font-size: 23px;
    line-height: 25px;
  }
}
@media (min-width: 1200px) {
  .h6,
  h6 {
    font-size: 37px;
    line-height: 39px;
  }
}
/* BLOCKS : Heading end */