/* common */
body{
  margin:0;
  padding:0;
  color: #121212;
}
*,:after,:before { 
  box-sizing: border-box;
}
.hidden {
    display: none !important;
}
[role="main"] .shopify-section {
  --section-is-first: 0;
  --hash-difference: calc(
    var(--section-background-hash) -
      var(--previous-section-background-hash, -1)
  );
  --hash-difference-abs: max(
    var(--hash-difference),
    -1 * var(--hash-difference)
  );
  --background-differs-from-previous: max(
    0,
    min(var(--hash-difference-abs, 1), 1)
  );
  background: rgb(var(--background));
  position: relative;
}
[role="main"] .shopify-section:not(.contents) {
  display: flow-root;
}
[role="main"] .shopify-section:empty {
  display: none;
}
.icon {
  fill: currentColor;
  display: inline-block;
  height: 30px;
  vertical-align: middle;
  width: 30px;
}
svg.icon:not(.icon--full-color) g {
  fill: inherit;
  stroke: inherit;
}
svg.icon:not(.icon--full-color) path {
  fill: inherit;
  stroke: inherit;
}
[tabindex="-1"]:focus {
  outline: 0 !important;
}
svg {
  vertical-align: middle;
  overflow: hidden;
}
svg:not(:root) {
  overflow: hidden;
}
[type=search] {
  -webkit-appearance: none;
  outline-offset: -2px;
}
img {
  border-style: none;
  vertical-align: middle;
  max-width: 100%;
  display: block;
}
button {
  border-radius: 0;
  font-family: inherit;
  font-size: inherit;
  line-height: inherit;
  margin: 0;
  overflow: visible;
  text-transform: none;
  -webkit-appearance: button;
}
button:focus {
  box-shadow: 0 0 0 0.2rem #4c7a8a80;
  outline: none;
  box-shadow: none !important;
  outline: 0 !important;
}
button:not(:disabled) {
  cursor: pointer;
}
input {
  font-family: inherit;
  font-size: inherit;
  line-height: inherit;
  margin: 0;
  overflow: visible;
}
[type=submit] {
  -webkit-appearance: button;
}
[type=submit]:not(:disabled) {
  cursor: pointer;
}
.form-control {
  background-clip: padding-box;
  background-color: #f5f5f6;
  border: 1px solid transparent;
  color: #494949;
  display: block;
  font-size: 1rem;
  font-weight: 400;
  height: auto;
  line-height: 1.5;
  padding: 13px 20px;
  transition: border-color 0.15s ease-in-out, box-shadow 0.15s ease-in-out;
  width: 100%;
}
.form-control:focus {
  background-color: #f5f5f6;
  border-color: #6c8dff;
  box-shadow: none;
  color: #494949;
  outline: 0;
}
.form-control::placeholder {
  color: #12121273;
  opacity: 1;
}
.form-control:disabled {
  background-color: #e9ecef;
  opacity: 1;
}
.form-control.navigation-form__field {
  background-color: #393d49;
  border: 2px solid #393d49;
  border-radius: 0;
  color: #fff;
  font-size: 16px;
  height: 50px;
  line-height: 1.5;
  padding: 0 18px;
}
.form-control.navigation-form__field::placeholder {
  color: #ffffffb3;
}
.form-control.navigation-form__field:focus {
  border-color: #fff;
}
.form-group {
  margin-bottom: 0.675rem;
  position: relative;
  text-align: left;
}
button.focus,
button:focus {
  box-shadow: none !important;
  outline: 0 !important;
}
.btn {
  background-color: #121212;
  border: 1.2px solid transparent;
  border-radius: 90px;
  color: #fff;
  display: inline-block;
  font-family: Sequel Sans Small Title, sans-serif;
  font-size: 13px;
  line-height: 20px;
  padding: 12px 32px;
  position: relative;
  text-align: center;
  transition: 0.15s cubic-bezier(0.42, 0, 0.13, 1.04);
  transition-property: background-color transform;
  -webkit-user-select: none;
  user-select: none;
  vertical-align: middle;
}
.ovel-button{
    border-radius: 3px;
}
.btn span {
  transform: scale(1);
  transition: transform 0.15s cubic-bezier(0.42, 0, 0.13, 1.04);
}
@media (min-width: 768px) {
  .btn {
    font-size: 16px;
    line-height: 1;
    padding: 14px 42px;
  }
}
@media (min-width: 992px) {
  .btn {
    line-height: 1;
    padding: 15px 53px;
  }
  .btn:not([disabled]):hover {
    color: #121212;
    text-decoration: none;
    transform: scale(0.98);
  }
  .btn:not([disabled]):hover span {
    display: inline-block;
    transform: scale(1.02);
  }
  .btn:not([disabled]):hover .btn__ui {
    color: #fff;
    transform: scale(1.02);
  }
}
.btn.disabled,
.btn:disabled {
  opacity: 0.65;
}
.btn .btn__ui {
  margin-left: 20px;
}
.btn .btn__ui .icon {
  fill: currentColor;
  height: 30px;
  width: 30px;
}
a.btn.disabled {
  pointer-events: none;
}
