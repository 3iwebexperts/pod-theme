}
.ajax-cart__item-remove,
.btn--underlined,
.btn-link,
.minimal-input-box__submit {
  background: none;
  border: none;
  border-bottom: 2px solid;
  border-radius: 0;
  color: #121212;
  display: inline-block;
  line-height: 1.5;
  padding: 0;
  position: relative;
  text-decoration: none;
}
.ajax-cart__item-remove:before,
.btn--underlined:before,
.btn-link:before,
.minimal-input-box__submit:before {
  border-bottom: 4px solid;
  content: "";
  display: block;
  left: 0;
  margin: auto;
  position: absolute;
  right: 0;
  top: 100%;
  transition: width 0.15s cubic-bezier(0.42, 0, 0.13, 1.04);
  width: 0;
}
.ajax-cart__item-remove:hover:before,
.btn--underlined:hover:before,
.btn-link:hover:before,
.minimal-input-box__submit:hover:before {
  border-bottom-width: 4px;
  width: 100%;
}
.ajax-cart__item-remove:focus,
.btn--underlined.focus,
.btn--underlined:focus,
.btn-link.focus,
.btn-link:focus,
.focus.ajax-cart__item-remove,
.focus.minimal-input-box__submit,
.minimal-input-box__submit:focus {
  box-shadow: none;
  text-decoration: none;
}
.ajax-cart__item-remove:disabled,
.btn--underlined.disabled,
.btn--underlined:disabled,
.btn-link.disabled,
.btn-link:disabled,
.disabled.ajax-cart__item-remove,
.disabled.minimal-input-box__submit,
.minimal-input-box__submit:disabled {
  color: #666;
  pointer-events: none;
}
